{
  "name": "@vercel/flags-core",
  "version": "0.1.8",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "next-js": {
        "import": "./dist/index.next-js.js",
        "require": "./dist/index.next-js.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./openfeature": {
      "next-js": {
        "import": "./dist/openfeature.next-js.js",
        "require": "./dist/openfeature.next-js.cjs"
      },
      "import": "./dist/openfeature.js",
      "require": "./dist/openfeature.cjs"
    }
  },
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "dist/*.d.ts",
        "dist/*.d.cts"
      ],
      "./openfeature": [
        "dist/openfeature.d.ts",
        "dist/openfeature.d.cts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "bin": {
    "vercel-flags": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "biome check",
    "test": "vitest --run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@vercel/functions": "^3.3.6",
    "commander": "14.0.2",
    "jose": "5.2.1",
    "js-xxhash": "4.0.0",
    "next-connection": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "20.11.17",
    "flags": "workspace:*",
    "msw": "2.6.4",
    "tsup": "8.5.1",
    "typescript": "5.6.3",
    "vite": "6.0.3",
    "vitest": "2.1.8"
  },
  "peerDependencies": {
    "@openfeature/server-sdk": "1.18.0",
    "flags": "*",
    "@vercel/flags-definitions": "*"
  },
  "peerDependenciesMeta": {
    "@openfeature/server-sdk": {
      "optional": true
    },
    "@vercel/flags-definitions": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
